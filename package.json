  "license": "MIT",
  "private": true,
  "scripts": {
    "build-server": "NODE_ENV=production webpack --config webpack.client.config.js -p",
    "build-client": "NODE_ENV=production webpack --config webpack.server.config.js -p",
    "build": "yarn build-server && yarn build-client",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start-dev": "rimraf dist && yarn build && yarn start",
    "dev-server": "webpack-dev-server --config webpack.client.config -d"
